<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.scode.kotlincrud">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".Crud" />
        <activity android:name="id.scode.oprekCrud.OprekCrud"></activity>
        <activity android:name="id.scode.progressBar.ProgressOprek"></activity>
        <activity android:name="id.scode.oprekCrud.ImageOprek">

        </activity>
        <activity android:name=".Treatment"></activity>
        <activity android:name=".Filtering"></activity>
        <activity android:name="id.scode.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>